@model List<TracNghiemOnline.Model.DanhGia>
@{
    ViewBag.Title = "DSDETHI";
    Layout = "~/Views/Share/LayOutHome.cshtml";
    var session = (TaiKhoan)Session[ComMonStants.UserLogin];

    List<Phong_Thi> phong_This = (List<Phong_Thi>)ViewBag.phong;
}
@using TracNghiemOnline.Modell.Dao;
@using TracNghiemOnline.Modell;
@using TracNghiemOnline.ComMon;

<div class="container py-8">
    <div class="card mb-4">
        <div class="card-body">

            <table class="table table-bordered" id="example" width="100%" cellspacing="0" style="font-family:Arial">
                <thead>
                    <tr>
                        <th> Phòng  </th>
                        <th>Mã Đề</th>
                        <th> Môn thi </th>
                        <th>Thời gian thi </th>

                        <th> Điểm </th>
                        <th> Ngày Thi </th>
                        <th> </th>
                    </tr>
                </thead>

                <tbody>
                    @{
                            int i = 0;
                            foreach (var item in Model.OrderByDescending(x => x.ketQuaThi.NgayThi))
                            {
                            

                            <tr>
                                <td>  @phong_This.ToList()[i].TenPhong </td>
                                <td> @item.ketQuaThi.Ma_DeThi </td>
                                <td>  @phong_This.ToList()[i].Bo_De.MonHoc.TenMon  </td>
                                <td> @phong_This.ToList()[i].Bo_De.ThoiGianThi </td>
                                <td>  @item.ketQuaThi.DiemSo </td>
                                <td>  @item.ketQuaThi.NgayThi.Value.Day/ @item.ketQuaThi.NgayThi.Value.Month/ @item.ketQuaThi.NgayThi.Value.Year </td>
                                <td> <a href="/TrangChu/TinhDiem/@item.ketQuaThi.Ma_DeThi" class="btn btn-info" style="padding: 1px 10px; color: #fff">Xem</a></td>
                            </tr>
                            i++;
                        }
                    }
                </tbody>

            </table>

        </div>

    </div>
</div>
