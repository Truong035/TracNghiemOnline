@model List<TracNghiemOnline.Modell.Phong_Thi>
@{
    ViewBag.Title = "DanhGiaKetQuahocTap";
    Layout = "~/Areas/Admin/Views/Shared/LaoutShare.cshtml";
}

<div class="container-fluid">

    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Mã Phòng </th>
                        <th>Lớp Học Phần </th>

                        <th></th>

                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>Mã Phòng </th>
                        <th>Lớp Học Phần </th>
                    
                        <th></th>
                    </tr>
                </tfoot>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td> @item.MaPhong </td>
                            <td> @item.LopHocPhan.TenLop </td>
                            <td>   <div class="col-4"><a href="/Admin/QuanLyThi/KetQuaphongthi/@item.MaPhong">  Xem Kết Qủa </a></div> </td>
                       
                        </tr>
                    }




                </tbody>

            </table>

        </div>
        <div class="row">
            <div class="col-3 " style="text-align:center">
                <a class="btn-dark nav-link" href="/Admin/QuanLyThi/CreateClassRoom" style="text-align:center">Tạo Phòng Thi</a>
            </div>
        </div>
    </div>
</div>


